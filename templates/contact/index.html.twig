{% extends 'base.html.twig' %}

{% block title %}Hello ContactController!{% endblock %}

{% block body %}
    <h2 class="title">Contactez-nous</h2>
    {{ form_start(form) }}
    {% for message in app.session.flashBag.get("message") %}
        <div class="alert {{ app.session.get("status") }}">{{ message }}</div>
    {% endfor %}
    {{ form_end(form) }}
{% endblock %}
